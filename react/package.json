{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@sentry/apm": "^5.20.1",
    "@sentry/cli": "^1.37.0",
    "@sentry/react": "^6.2.1",
    "@sentry/tracing": "^6.2.1",
    "history": "^1.17.0",
    "material-ui": "^0.18.7",
    "nanomatch": "^1.2.13",
    "posix-character-classes": "^1.0.0",
    "react": "^16.13.1",
    "react-bootstrap": "^1.5.2",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router": "^1.0.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "1.0.10",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "request": "^2.88.0",
    "serve": "^12.0.0",
    "snapdragon": "^0.12.0",
    "snapdragon-node": "^3.0.0",
    "split-string": "^6.1.0",
    "to-regex": "^3.0.2"
  },
  "scripts": {
    "predeploy": "npm run build && make setup_release",
    "deploy": "serve -s build",
    "deploylocal": "npm run buildlocal && make setup_release && serve -s build",
    "start": "REACT_APP_RELEASE=`../calver.sh` react-scripts start",
    "build": "REACT_APP_RELEASE=`../calver.sh` react-scripts build",
    "buildlocal": "REACT_APP_RELEASE=`../calver.sh` REACT_APP_BACKEND_LOCAL=http://localhost:8080 react-scripts build",
    "eject": "react-scripts eject",
    "deploy-ghp": "npm run predeploy && gh-pages -d build"
  },
  "devDependencies": {
    "gh-pages": "^1.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
